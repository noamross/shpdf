{
  "name": "shpdf",
  "author": "Noam Ross",
  "description": "Goes Straight to Sci-Hub PDF iframe source",

  "manifest_version": 2,
  "version": "1.2",
  "permissions": [
    "*://sci-hub.tw/*",
    "*://sci-hub.love/*",
    "*://sci-hub.nu/*"
    ],
  "content_scripts": [
  {
    "matches": [
      "*://sci-hub.tw/*",
      "*://sci-hub.love/*",
      "*://sci-hub.nu/*"
      ],
    "js": ["shpdf.js"],
    "run_at": "document_start"
  }
  ]
}
