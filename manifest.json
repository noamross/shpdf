{
  "name": "shpdf",
  "author": "Noam Ross",
  "description": "Goes Straight to Sci-Hub PDF iframe source",

  "manifest_version": 2,
  "version": "1.2",
  "permissions": [
    "http*://sci-hub.tw/*",
    "http*://sci-hub.love/*",
    "http*:/sci-hub.nu/*",
    "http*://sci-hub.tw/*",
    "http*://sci-hub.tw/*",
    "http*://sci-hub.nu/*"
    ],
  "content_scripts": [
  {
    "matches": [
      "http*://sci-hub.tw/*",
      "http*://sci-hub.love/*",
      "http*:/sci-hub.nu/*",
      "http*://sci-hub.tw/*",
      "http*://sci-hub.tw/*",
      "http*://sci-hub.nu/*"
      ],
    "js": ["shpdf.js"],
    "run_at": "document_start"
  }
  ]
}
